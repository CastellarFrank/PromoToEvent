        <script type="text/javascript">
            var functionMessageQuestion = null;

            function yesOrNoMessageQuestion(title, content, actionFunction) {
                $("#bModalTitle").text(title);
                $("#bModalContent").text(content);
                functionMessageQuestion = actionFunction;
                $("#questionActivator").click();
            }

            $(document).ready(function () {

                $("#bModalContentYesButton").on("click", function(e) {
                    functionMessageQuestion();
                });

                //start Edit
                $(":button.toPopUp").on("click", function (e) {
                    var uri = $(this).attr("id");
                    $.ajax({
                            type: "GET",
                            url: uri
                        })
                        .done(function (html) {
                            $("#EditPopUpModalBody").replaceWith(html);
                            $("#EditPopUpModalActivator").click();
                    });
                });
                //end edit

                //start Delete
                $(":button.toDelete").on("click", function () {
                    var uri = $(this).attr("id");
                    var idMessage = "input[id*='" + uri + "_Message']";
                    var idForm = "#Form_" + uri;
                    var message = $(idMessage).val();

                    yesOrNoMessageQuestion("Eliminar", message, function () {
                        $(idForm).submit();
                    });
                }); //End of delete


                $('#Form').on("submit",function (e) {
                    alert(form.attr('action') + form.serialize());
                    $.post(form.attr('action'),form.serialize());
                    return false;
                });



            });

        </script>    
    
